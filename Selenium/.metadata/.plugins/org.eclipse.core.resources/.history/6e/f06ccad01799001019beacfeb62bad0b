	//		 -------------------------Change Product status--------------------------
			       
			       public void ChangeProductStatus() throws InterruptedException {
			    	   
			    	   Thread.sleep(3000);
			       
					WebElement status = driver.findElement(By.xpath("(//select[@class='grid-select'])[2]"));

					Select statusSelect = new Select(status);

					statusSelect.selectByVisibleText("Initiated");
					
					Thread.sleep(2000);
					
					 driver.findElement(By.xpath("(//div[@class=\"grid-actions-dropdown dropdown\"])[1]")).click();
				        Thread.sleep(2000);
				        
				        driver.findElement(By.xpath("(//a[@role='button'])[2]")).click();
				        
				        Thread.sleep(2000);
				        
				        driver.findElement(By.xpath("//div[@class='product-variant-dropdown dropdown']")).click();
				        
				        Thread.sleep(2000);
				        
				        driver.findElement(By.xpath("(//a[@role='button'])[1]")).click();
				        Thread.sleep(2000);
				        
				        WebElement changeStatus = driver.findElement(By.name("selectedStatus"));

						Select statusSelect1 = new Select(changeStatus);

						statusSelect1.selectByVisibleText("Active"); 
						
						Thread.sleep(2000);
						
						driver.findElement(By.xpath("//button[contains(text(),'Submit')]")).click();
						
						Thread.sleep(1000);
						
						driver.findElement(By.xpath("//button[contains(text(),'OK')]")).click();
			
			       }				
	//			----------------------Edit Product------------------------------------------
			       
						public void EditProduct() throws InterruptedException {
						Thread.sleep(2000);
				        
						WebElement status1 = driver.findElement(By.xpath("//select[@class='grid-select e-focus']"));

						Select statusSelect12 = new Select(status1);

						statusSelect12.selectByVisibleText("Active");
					
						Thread.sleep(2000);
						
						   driver.findElement(By.xpath("(//div[@class=\"grid-actions-dropdown dropdown\"])[1]")).click();
